{
  "name": "jsreport-public-templates",
  "version": "0.2.0",
  "description": "jsreport extension for granting public access to particular templates",
  "scripts": {
    "test": "mocha test --timeout=5000"
  },
  "homepage": "https://github.com/jsreport/jsreport-public-templates",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-public-templates.git"
  },
  "keywords": [
    "jsreport"
  ],
  "standard": {
    "ignore": [
      "public/**"
    ],
    "globals": [
      "describe",
      "it",
      "beforeEach",
      "define",
      "ace"
    ]
  },
  "license": "LGPL",
  "dependencies": {
    "q": "1.4.1",
    "string": "3.3.1",
    "underscore": "1.8.3",
    "uuid": "2.0.1"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "eslint": "^1.9.0",
    "mocha": "^2.3.3",
    "should": "^7.1.1",
    "standard": "^5.3.1",
    "supertest": "^1.1.0",
    "jsreport-core": "jsreport/jsreport-core",
    "jsreport-templates": "jsreport/jsreport-templates",
    "jsreport-authorization": "jsreport/jsreport-authorization",
    "jsreport-authentication": "jsreport/jsreport-authentication",
    "jsreport-express": "jsreport/jsreport-express"
  }
}
